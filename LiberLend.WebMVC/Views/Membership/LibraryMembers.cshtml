@model IEnumerable<LiberLend.Models.MembershipModels.MembershipListItem>

@{
    ViewBag.Title = "Library Members";
}

<h2>Library Members</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ApplicationUserName)
        </th>
        @*<th>
                @Html.DisplayNameFor(model => model.IsAuthorized)
            </th>*@
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ApplicationUserName)
            </td>
            @*<td>
                    @Html.DisplayFor(modelItem => item.IsAuthorized)
                </td>*@
        </tr>
    }

</table>
@Html.ActionLink("Back to Libraries", "../Library/Index")
