
@{
    ViewBag.Title = "Index";
}

<h2>
    @Html.ActionLink("View Incoming Reservations", "IndexAsOwner");
</h2>
<h2>
    @Html.ActionLink("View Outgoing Reservations", "IndexAsBorrower");
</h2>

@if (TempData.ContainsKey("SaveResult"))
{
    <div class="alert alert-success fade in" role="alert">
        <button type="button" class="close" data-dismiss="alert">
            <span>&times;</span>
        </button>
        @TempData["SaveResult"]
    </div>
}

